<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.happy.model.repo.StoreRepo">

	<select id="storeList" parameterType="string" resultType="Store">
		SELECT *
		FROM storeinfo WHERE category = #{category} and dongCode = #{dongCode}
	</select>

</mapper>